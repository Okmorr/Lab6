<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 	Height="450" Width="450"
					MaxHeight="450" MinWidth="450"
					MinHeight="450" MaxWidth="450"
             mc:Ignorable="d" d:DesignWidth="450" d:DesignHeight="450"
             x:Class="Lab6.Views.NoteView">
	<UserControl.Styles>
		<StyleInclude Source="/Styles/Styles.axaml"/>
	</UserControl.Styles>


	<Grid Classes="NoteBook"  ColumnDefinitions="*" RowDefinitions="50, *, 50, 50">
		<TextBox Name="Header" Text="{Binding Header}" Classes="gridTextBox"/>
		<TextBox Name="Text" Classes="gridTextBox"/>
		<Button Name="Ok" Classes="gridBtn"/>
		<Button Name="Cancel" Classes="gridBtn"/>
	</Grid>

</UserControl>
